<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="OnRibbonLoaded">
  <ribbon>
    <contextualTabs>
      <tabSet idMso="TabSetAppointment">
        <tab idMso="TabAppointment">
          <group id="group3" getLabel="Group_GetLabel" centerVertically="true">
            <button id="label3" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" getImage="OnGetImage" getShowImage="OnGetShowImage" />
            <button id="label33" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" getImage="OnGetImage" getShowImage="OnGetShowImage" onAction="OnAction" />
            <button id="label333" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" getImage="OnGetImage" getShowImage="OnGetShowImage" />
            <button id="label3333" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" getImage="OnGetImage" getShowImage="OnGetShowImage" />
            <button id="directAccept3" size="large" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" imageMso="AcceptInvitation" onAction="OnAction" />
          </group>
        </tab>
      </tabSet>
      <tabSet idMso="TabSetReccurringAppointment">
        <tab idMso="TabRecurringAppointment">
          <group id="group4" getLabel="Group_GetLabel" centerVertically="true">
            <button id="label4" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" getImage="OnGetImage" getShowImage="OnGetShowImage" />
            <button id="label44" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" getImage="OnGetImage" getShowImage="OnGetShowImage" onAction="OnAction" />
            <button id="label444" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" getImage="OnGetImage" getShowImage="OnGetShowImage" />
            <button id="label4444" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" getImage="OnGetImage" getShowImage="OnGetShowImage" />
            <button id="directAccept4" size="large" getLabel="OnGetLabel" getVisible="OnGetVisible" getScreentip="OnGetScreentip" getSupertip="OnGetSupertip" imageMso="AcceptInvitation" onAction="OnAction" />
          </group>
        </tab>
      </tabSet>
    </contextualTabs>
  </ribbon>
  <backstage>
    <tab id="TabMeetingInfo" getLabel="TabMeetingInfo_GetLabel" columnWidthPercent="30" insertAfterMso="TabPrint" visible="true">
      <firstColumn>
        <group id="grpSeperator">
        </group>
        <group id="GroupSettings">
          <primaryItem>
            <button id="ButtonSettings" getLabel="Group_GetLabel" onAction="OnAction"/>
          </primaryItem>
        </group>
        <group id="GroupCredits">
          <primaryItem>
            <button id="ButtonCredits" getLabel="Group_GetLabel" onAction="OnAction"/>
          </primaryItem>
        </group>
      </firstColumn>
      <secondColumn>
        <group id="GroupInfo" getLabel="Group_GetLabel">
          <topItems>
            <labelControl id="lblInfo" getLabel="Group_GetLabel" />
            <hyperlink id="linkDownload" label="MeetingInfo" target="https://github.com/LukasWestholt/MeetingInfo" />
            <hyperlink id="linkMail" label="Mail me" target="mailto:support@lukaswestholt.de" />
          </topItems>
        </group>
      </secondColumn>
    </tab>
  </backstage>
</customUI>